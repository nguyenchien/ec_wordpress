    url('../../../../../images/fonts/navicon.svg?6ggsos#navicon') format('svg');
    font-weight: normal;
    font-style: normal;
    font-display: block;
}
[class^="navicon-"], [class*=" navicon-"] {
    font-family: 'navicon',serif !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
.navicon-faq:before {
    content: "\e900";
    color: #fff;
}
.navicon-flower:before {
    content: "\e901";
    color: #fff;
}
.navicon-lock:before {
    content: "\e902";
    color: #fff;
}
.navicon-note:before {
    content: "\e903";
    color: #fff;
}
.navicon-phone:before {
    content: "\e904";
    color: #fff;
}